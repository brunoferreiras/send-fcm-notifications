{
  "name": "send-fcm-notifications",
  "version": "1.0.0",
  "description": "A API to send fcm notifications by Firebase HTTP v1 API.",
  "main": "./bin/server.js",
  "scripts": {
    "start": "nodemon --exec babel-node --presets env,stage-2 ./src/app.js",
    "prepush": "npm run lint && npm run test:coverage",
    "build": "babel ./src -d ./build ",
    "prebuild": "rm -rf build/*",
    "production": "node ./build/app.js",
    "preproduction": "npm run build",
    "lint": "./node_modules/.bin/eslint src/**/*.js",
    "lint:tests": "./node_modules/.bin/eslint tests/**/*.js",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --require dotenv/config --exit",
    "test:tdd": "npm test -- --watch",
    "test:coverage": "nyc npm run test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "**/*.spec.js",
      "build"
    ]
  },
  "keywords": [
    "notifications",
    "hostweb"
  ],
  "author": "Bruno Ferreira <fs.brunoferreira@gmail.com> (https://www.linkedin.com/in/brunoferreiras/)",
  "license": "MIT",
  "dependencies": {
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "cors": "2.8.5",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "googleapis": "37.2.0",
    "joi": "14.3.1",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "3.7.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.2.0",
    "chai-http": "4.2.1",
    "eslint": "5.14.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "husky": "1.3.1",
    "mocha": "6.0.1",
    "nodemon": "1.18.10",
    "nyc": "13.3.0"
  }
}
